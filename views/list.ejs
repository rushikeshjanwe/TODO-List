<%- include("header") -%>
  <div class="box" id="heading">
    <h1> <%=listTitle %> </h1>
  </div>

  <div class="box">
    <%  for(var i = 0; i < itemList.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p> <%= itemList[i] %> </p>
    </div>
    <% } %>

    <form action="/" method="post">
      <input type="text" name="item1" autocomplete="off">
      <button type="submit" name="button" value="<%= listTitle%>">+</button>
    </form>
  </div>

  <%- include("footer") -%>
